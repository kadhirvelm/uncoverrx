{
    "name": "@cohortrx-user/backend",
    "version": "1.0.0",
    "description": "The backend",
    "main": "dist/index.js",
    "repository": "https://github.com/kadhirvelm/cohortrx",
    "type": "module",
    "private": true,
    "dependencies": {
        "@cohortrx-user/api": "1.0.0",
        "@cohortrx-user/prisma-client": "1.0.0",
        "@yarnpkg/pnpify": "^4.0.0-rc.43",
        "body-parser": "^1.20.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "lodash": "^4.17.21",
        "node-cron": "^3.0.2",
        "openapi-fetch": "^0.1.4",
        "pg": "^8.11.0",
        "pg-hstore": "^2.3.4",
        "uuid": "^9.0.0"
    },
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "start": "node --experimental-modules --experimental-specifier-resolution=node dist/index.js",
        "start:watch": "nodemon --experimental-modules --experimental-specifier-resolution=node dist/index.js --config=./nodemon.json",
        "profile": "node --experimental-modules --experimental-specifier-resolution=node --prof dist/index.js",
        "clinic": "clinic flame -- node --experimental-modules --experimental-specifier-resolution=node dist/index.js",
        "dev": "yarn build:watch & yarn start:watch",
        "lint": "eslint . --ext .ts,.tsx --cache",
        "lint:fix": "yarn lint --fix",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest --colors --detectOpenHandles"
    },
    "engines": {
        "node": "^18.12.1"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.1",
        "@types/lodash": "^4.14.194",
        "@types/node": "^20.1.7",
        "@types/node-cron": "^3.0.7",
        "@types/pg": "^8.6.6",
        "@types/request": "^2.48.8",
        "@types/sequelize": "^4.28.15",
        "@types/uuid": "^9.0.1",
        "@types/validator": "^13.7.17",
        "jest": "^29.5.0",
        "nodemon": "^2.0.22",
        "prisma": "4.14.0",
        "reflect-metadata": "0.1.13",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5",
        "webpack": "^5.82.1"
    }
}
